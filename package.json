{
  "name": "kmmit",
  "version": "1.0.2",
  "description": "CLI tool for automated git commits and branch naming with GitHub Copilot",
  "main": "dist/index.js",
  "bin": {
    "kmit": "dist/index.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sudoDevesh/kmit.git"
  },
  "homepage": "https://github.com/sudoDevesh/kmit#readme",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "git",
    "git-cli",
    "commit",
    "commit-message",
    "conventional-commits",
    "branch-naming",
    "developer-tools",
    "cli",
    "automation",
    "productivity",
    "github",
    "github-copilot",
    "workflow",
    "dev-experience"
  ],
  "engines": {
    "node": ">=18"
  },
  "preferGlobal": true,
  "author": "Devesh Korde",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "inquirer": "^13.0.2",
    "simple-git": "^3.30.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
